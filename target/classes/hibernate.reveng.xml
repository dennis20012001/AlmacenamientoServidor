<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-schema="almacen" match-name=".*" />
	<table-filter match-catalog="almacen" match-name="invoice_details" />
	<table-filter match-catalog="almacen" match-name="invoices" />
	<table-filter match-catalog="almacen" match-name="products" />
	<table-filter match-catalog="almacen" match-name="users" />
<table catalog="almacen" name="invoice_details">
	<column name="detail_id"></column>
	<column name="invoice_id"></column>
	<column name="product_id"></column>
	<column name="quantity"></column>
	<column name="paid"></column>
</table>
<table catalog="almacen" name="invoices">
	<column name="invoice_id"></column>
	<column name="id_user"></column>
</table>
<table catalog="almacen" name="products">
	<column name="product_id"></column>
	<column name="image"></column>
	<column name="name"></column>
	<column name="amount"></column>
	<column name="productLimit"></column>
	<column name="season"></column>
	<column name="enabled"></column>
</table>
<table catalog="almacen" name="users">
	<column name="id_user"></column>
	<column name="name"></column>
	<column name="mail"></column>
	<column name="image"></column>
	<column name="pass"></column>
	<column name="rol"></column>
</table>
</hibernate-reverse-engineering>